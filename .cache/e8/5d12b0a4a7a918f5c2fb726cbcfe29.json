olumn":22}},{"generated":{"line":53,"column":24},"source":"main/main.js","original":{"line":58,"column":24}},{"generated":{"line":54,"column":2},"source":"main/main.js","original":{"line":59,"column":4},"name":"controls"},{"generated":{"line":54,"column":10},"source":"main/main.js","original":{"line":59,"column":12}},{"generated":{"line":54,"column":11},"source":"main/main.js","original":{"line":59,"column":13},"name":"update"},{"generated":{"line":54,"column":17},"source":"main/main.js","original":{"line":59,"column":19}},{"generated":{"line":54,"column":19},"source":"main/main.js","original":{"line":59,"column":21}},{"generated":{"line":55,"column":2},"source":"main/main.js","original":{"line":60,"column":4},"name":"renderer"},{"generated":{"line":55,"column":10},"source":"main/main.js","original":{"line":60,"column":12}},{"generated":{"line":55,"column":11},"source":"main/main.js","original":{"line":60,"column":13},"name":"render"},{"generated":{"line":55,"column":17},"source":"main/main.js","original":{"line":60,"column":19}},{"generated":{"line":55,"column":18},"source":"main/main.js","original":{"line":60,"column":20},"name":"scene"},{"generated":{"line":55,"column":23},"source":"main/main.js","original":{"line":60,"column":25}},{"generated":{"line":55,"column":25},"source":"main/main.js","original":{"line":60,"column":27},"name":"camera"},{"generated":{"line":55,"column":31},"source":"main/main.js","original":{"line":60,"column":33}},{"generated":{"line":55,"column":32},"source":"main/main.js","original":{"line":60,"column":34}},{"generated":{"line":56,"column":0},"source":"main/main.js","original":{"line":61,"column":0}}],"sources":{"main/main.js":"import * as THREE from 'three'\r\nimport { OrbitControls } from 'three/examples/jsm/controls/OrbitControls'\r\nlet scene, camera, renderer,container\r\nlet controls\r\ninit()\r\ninitMeshes()\r\n/**\r\n    多相机的使用：\r\n    1.创建Mesh\r\n    3.增加相机\r\n    4.设置视口\r\n    5.设置裁剪区域\r\n    6.添加正交相机\r\n\r\n \r\n */\r\n\r\n\r\nfunction init() {\r\n\r\n    scene = new THREE.Scene()\r\n    scene.background=new THREE.Color(0x888888)\r\n    scene.add(new THREE.AxesHelper(300))\r\n\r\n    container = document.getElementById('container')\r\n    renderer = new THREE.WebGLRenderer()\r\n    renderer.setPixelRatio(window.devicePixelRatio)\r\n    renderer.setSize(window.innerWidth, window.innerHeight)\r\n    container.appendChild(renderer.domElement)\r\n    renderer.setAnimationLoop(animate)\r\n\r\n    // 相机1\r\n    camera = new THREE.PerspectiveCamera(50, window.innerWidth / window.innerHeight, 1, 3000)\r\n    camera.position.set(1000,1000,1000)\r\n\r\n    // 摄像机视锥体的长宽比\r\n    camera.aspect =window.innerWidth / window.innerHeight\r\n    camera.updateProjectionMatrix();\r\n\r\n\r\n    //轨道控制器\r\n    controls = new OrbitControls(camera, renderer.domElement)\r\n    controls.enableDamping = true\r\n    controls.dampingFactor = 0.05\r\n    window.addEventListener('resize', onWindowResize)\r\n}\r\n\r\nfunction initMeshes() {\r\n\r\n}\r\n\r\nfunction onWindowResize() {\r\n    camera.aspect = window.innerWidth / window.innerHeight\r\n    camera.updateProjectionMatrix()\r\n    renderer.setSize(window.innerWidth, window.innerHeight)\r\n}\r\n\r\nfunction animate(times) {\r\n    controls.update()\r\n    renderer.render(scene, camera);\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"749f3f0d414d7b75aa4ced9ac20b9466","cacheData":{"env":{}}}